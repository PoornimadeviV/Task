#include <iostream>
using namespace std;

int fibonacci(int n) {
    if (n <= 1) return n; 
F(0) = 0, F(1) = 1
    return fibonacci(n - 1) + fibonacci(n - 2); 
}

int main() {
    int n;
    cout << "Enter the position (n): ";
    cin >> n;
   cout << "The " << n << "th Fibonacci number is " << fibonacci(n) << endl;
   return 0;
}
